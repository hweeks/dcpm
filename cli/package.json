{
  "name": "@dcpm/cli",
  "version": "2.5.1",
  "description": "The cli for dcpm",
  "author": "Harris Weeks <root@hweeks.com>",
  "homepage": "",
  "license": "ISC",
  "bin": {
    "dcpm": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": "git@github.com:hweeks/dcpm.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist/ && yarn build",
    "build": "tsc",
    "watch": "tsc -w",
    "watch:brk": "TS_NODE_FILES=true node --inspect-brk=127.0.0.1:9222 -r ts-node/register ./lib/index.ts",
    "dev": "nodemon",
    "semantic-release": "semantic-release",
    "rel:debug": "node --inspect-brk $(npm bin)/semantic-release -e semantic-release-monorepo -d --ci",
    "lint": "eslint lib/**/*.ts",
    "test": "jest",
    "test:debug": "node --inspect-brk $(npm bin)/jest --watchAll --no-cache --runInBand"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "chalk": "^3.0.0",
    "form-data": "^3.0.0",
    "glob": "^7.1.6",
    "inquirer": "^7.0.1",
    "js-yaml": "^3.13.1",
    "mustache": "^4.0.0",
    "node-fetch": "^2.6.0",
    "semver": "^7.1.1",
    "unzipper": "^0.10.5",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.3",
    "@types/archiver": "3.1.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "25.1.2",
    "@types/js-yaml": "3.12.2",
    "@types/mustache": "4.0.1",
    "@types/node": "12.12.29",
    "@types/node-fetch": "2.5.4",
    "@types/semver": "6.2.0",
    "@types/unzipper": "0.10.1",
    "@types/yargs": "13.0.4",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "eslint": "6.6.0",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "semantic-release": "17.0.4",
    "ts-jest": "25.2.0",
    "ts-node": "8.5.4",
    "typescript": "3.7.4"
  }
}
