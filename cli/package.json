{
  "name": "@dcpm/cli",
  "version": "2.5.1",
  "description": "The cli for dcpm",
  "author": "Harris Weeks <root@hweeks.com>",
  "homepage": "",
  "license": "ISC",
  "bin": {
    "dcpm": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": "git@github.com:hweeks/dcpm.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist/ && yarn build",
    "build": "tsc",
    "watch": "tsc -w",
    "watch:brk": "TS_NODE_FILES=true node --inspect-brk=127.0.0.1:9222 -r ts-node/register ./lib/index.ts",
    "dev": "nodemon",
    "semantic-release": "semantic-release",
    "rel:debug": "node --inspect-brk $(npm bin)/semantic-release -e semantic-release-monorepo -d --ci",
    "lint": "eslint lib/**/*.ts",
    "test": "jest",
    "test:debug": "node --inspect-brk $(npm bin)/jest --watchAll --no-cache --runInBand"
  },
  "dependencies": {
    "archiver": "3.1.1",
    "chalk": "3.0.0",
    "cli-table": "0.3.1",
    "form-data": "3.0.1",
    "glob": "7.1.6",
    "inquirer": "7.0.1",
    "js-yaml": "3.13.1",
    "mustache": "4.0.0",
    "node-fetch": "2.6.0",
    "semver": "7.1.1",
    "unzipper": "0.10.11",
    "yargs": "14.2.0"
  },
  "devDependencies": {
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.0",
    "@types/archiver": "3.1.0",
    "@types/cli-table": "0.3.0",
    "@types/glob": "7.2.0",
    "@types/inquirer": "6.5.0",
    "@types/jest": "27.4.0",
    "@types/js-yaml": "3.12.3",
    "@types/mustache": "4.1.2",
    "@types/node": "17.0.6",
    "@types/node-fetch": "2.5.6",
    "@types/semver": "6.2.1",
    "@types/unzipper": "0.10.3",
    "@types/yargs": "13.0.8",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "eslint": "8.6.0",
    "jest": "27.4.5",
    "nodemon": "2.0.15",
    "semantic-release": "17.0.4",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
