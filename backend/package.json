{
  "name": "@dcpm/backend",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "Harris Weeks <root@hweeks.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/server/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --maxWorkers=3 --forceExit",
    "build": "rimraf dist && tsc --project tsconfig-build.json",
    "watch": "TS_NODE_FILES=true node --inspect=0.0.0.0 -r ts-node/register lib/server/index.ts",
    "watch:brk": "TS_NODE_FILES=true node --inspect-brk=0.0.0.0 -r ts-node/register lib/server/index.ts",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build -d",
    "dev:down": "docker-compose stop",
    "dev:logs": "docker-compose logs --tail 100 -f backend",
    "dev:sh": "docker-compose exec backend sh",
    "dev:start": "nodemon",
    "lint": "eslint lib/**/*.ts"
  },
  "dependencies": {
    "bcrypt": "3.0.8",
    "body-parser": "1.19.0",
    "bson": "4.6.0",
    "bunyan": "1.8.15",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "gridfs-stream": "1.1.1",
    "helmet": "3.23.3",
    "jws": "3.2.2",
    "mongodb": "3.7.3",
    "mongoose": "5.13.13",
    "multer": "1.4.2",
    "multer-gridfs-storage": "4.0.1",
    "semver": "6.3.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "2.1.1",
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.19.2",
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.4",
    "@types/gridfs-stream": "0.5.32",
    "@types/helmet": "0.0.45",
    "@types/jest": "^27.0.3",
    "@types/jws": "3.2.1",
    "@types/mongodb": "3.6.20",
    "@types/multer": "1.4.7",
    "@types/node": "12.20.37",
    "@types/semver": "6.2.1",
    "@types/uuid": "3.4.8",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "jest": "27.4.5",
    "mongodb-memory-server": "8.0.4",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.1",
    "ts-node": "8.8.2",
    "typescript": "3.9.10"
  }
}
