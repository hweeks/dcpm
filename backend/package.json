{
  "name": "@dcpm/backend",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "Harris Weeks <root@hweeks.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/server/index.js",
  "directories": {
    "lib": "lib",
    "test": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --runInBand --forceExit --detectOpenHandles",
    "build": "tsc",
    "watch": "TS_NODE_FILES=true node --inspect=0.0.0.0 -r ts-node/register lib/server/index.ts",
    "watch:brk": "TS_NODE_FILES=true node --inspect-brk=0.0.0.0 -r ts-node/register lib/server/index.ts",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build -d",
    "dev:down": "docker-compose stop",
    "dev:logs": "docker-compose logs --tail 100 -f backend",
    "dev:sh": "docker-compose exec backend sh",
    "dev:start": "nodemon",
    "lint": "eslint lib/**/*.ts"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "express": "^4.17.1",
    "gridfs-stream": "^1.1.1",
    "helmet": "^3.21.2",
    "jws": "^3.2.2",
    "mongodb": "3.5.6",
    "mongoose": "^5.7.10",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.1",
    "semver": "^6.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "1.1.5",
    "@types/bcrypt": "3.0.0",
    "@types/bunyan": "1.8.6",
    "@types/express": "4.17.4",
    "@types/gridfs-stream": "0.5.32",
    "@types/helmet": "0.0.45",
    "@types/jest": "24.9.1",
    "@types/jws": "3.2.1",
    "@types/mongoose": "5.7.11",
    "@types/multer": "1.3.10",
    "@types/node": "12.12.35",
    "@types/semver": "6.2.1",
    "@types/uuid": "3.4.8",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "jest": "25.3.0",
    "jest-environment-node": "25.3.0",
    "nodemon": "1.19.4",
    "ts-jest": "25.4.0",
    "ts-node": "8.8.2",
    "typescript": "3.8.3"
  }
